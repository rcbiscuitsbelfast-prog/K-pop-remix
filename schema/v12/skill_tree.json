{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SkillTree v12",
  "type": "object",
  "properties": {
    "skills": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {"type":"string"},
          "name": {"type":"string"},
          "requires": {"type":"array", "items": {"type":"string"}}
        },
        "required": ["id", "name"]
      }
    }
  },
  "required": ["skills"]
}